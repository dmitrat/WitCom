<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
	  <TargetFrameworks>net9.0-windows;net8.0-windows;net7.0-windows;net6.0-windows;net5.0-windows</TargetFrameworks>
    <Nullable>enable</Nullable>
	<UseWPF>true</UseWPF>

	  <SignAssembly>False</SignAssembly>
	  <RootNamespace>OutWit.InterProcess.Agent</RootNamespace>

	  <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
	  <PackageId>OutWit.InterProcess.Agent</PackageId>
	  <Version>1.0.0</Version>
	  <Authors>Dmitry Ratner</Authors>
	  <Company>OutWit</Company>
	  <Product>OutWit.InterProcess.Agent</Product>
	  <Description>Inter Process Agent logic</Description>
	  <PackageTags>OutWit;Communication;InterProcess;Agent</PackageTags>
	  <Copyright>Copyright © Dmitry Ratner 2025</Copyright>

	  <RepositoryUrl>https://github.com/dmitrat/WitCom/tree/main/InterProcess/OutWit.InterProcess.Agent</RepositoryUrl>
	  <PackageProjectUrl>https://github.com/dmitrat/WitCom/tree/main/InterProcess/OutWit.InterProcess.Agent</PackageProjectUrl>

	  <PackageIcon>OutWitIcon128.png</PackageIcon>
	  <PackageLicenseFile>LICENSE</PackageLicenseFile>

  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="OutWit.Communication.Server" Version="1.0.0" />
    <PackageReference Include="OutWit.InterProcess" Version="1.0.0" />
  </ItemGroup>

	<ItemGroup>
		<None Include="LICENSE" Pack="true" PackagePath="\" />
	</ItemGroup>

	<ItemGroup>
		<None Include="OutWitIcon128.png" Pack="true" PackagePath="\" />
	</ItemGroup>

</Project>
